{
	"Create a new PDF dictionary object": {
		"description": "Insert an empty PDF dictionary object",
		"prefix": "obj",
		"body": [
			"0 obj",
			"<<",
			"    /Type ${1:/}",
			">>",
			"endobj",
			""
		]
	},
	"Creat a new PDF stream object": {
		"description": "Insert an empty PDF stream object",
		"prefix": "stream",
		"body": [
			"0 obj",
			"<<",
			"    /Length ${1:0}",
			"    /Filter null",
			">>",
			"stream",
			"",
			"endstream",
			"endobj",
			""
		]
	},
	"Create a minimal empty PDF file" : {
		"description": "A minimal empty PDF file",
		"prefix": "PDF-",
		"body":[
			"%PDF-1.7",
			"%%╬╝ß┐ª",
			"",
			"1 0 obj",
			"<<",
			"    /Type /Catalog",
			"    /Outlines 2 0 R",
			"    /Pages 3 0 R",
			">>",
			"endobj",
			"",
			"2 0 obj",
			"<<",
			"    /Type /Outlines",
			"    /Count 0",
			">>",
			"endobj",
			"",
			"3 0 obj % Page Tree Root",
			"<<",
			"    /Type /Pages",
			"    /Kids [ 4 0 R ]",
			"    /Count 1",
			">>",
			"endobj",
			"",
			"4 0 obj % Single Page",
			"<<",
			"    /Type /Page",
			"    /Parent 3 0 R",
			"    /MediaBox [ 0 0 1000 1000 ]",
			"    /Contents 5 0 R",
			"    /Resources << >>",
			">>",
			"endobj",
			"",
			"5 0 obj % Page content stream",
			"<<",
			"    /Length 0",
			">>",
			"stream",
			"q",
			"Q",
			"endstream",
			"endobj",
			"xref",
			"0 6",
			"0000000000 65535 f",
			"0000000021 00000 n",
			"0000000107 00000 n",
			"0000000169 00000 n",
			"0000000266 00000 n",
			"0000000419 00000 n",
			"trailer",
			"<< /Root 1 0 R /Size 6 >>",
			"startxref",
			"502",
			"%%EOF",
			""
		]
	}
}