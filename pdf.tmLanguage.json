{
	"name": "PDF",
	"scopeName": "source.pdf",
	"patterns": [
		{
			"name": "comment.line.percentage.pdf",
			"match": "%.*"
		},
		{
			"name": "string.quoted.pdf",
			"begin": "\\(",
			"end": "\\)",
			"patterns": [
				{
					"name": "constant.character.escape.octal.pdf",
					"match": "\\\\\\d{3}(?=[^\\d]|$)"
				},
				{
					"name": "constant.character.escape.newline.pdf",
					"match": "\\\\n"
				},
				{
					"name": "constant.character.escape.return.pdf",
					"match": "\\\\r"
				},
				{
					"name": "constant.character.escape.tab.pdf",
					"match": "\\\\t"
				},
				{
					"name": "constant.character.escape.backslash.pdf",
					"match": "\\\\\\\\"
				},
				{
					"name": "constant.character.escape.other.pdf",
					"match": "\\\\."
				}
			]
		},
		{
			"name": "string.quoted.hex.pdf",
			"match": "<[^>]*>"
		},
		{
			"name": "keyword.control.pdf",
			"match": "\\b(?:\\d+ \\d+ R|startxref|xref|trailer|null|true|false|^\\d+ \\d+ obj|^endobj|endstream|stream)\\b"
		},
		{
			"name": "punctuation.definition.dictionary.pdf",
			"begin": "<<",
			"end": ">>",
			"patterns": [
				{
					"name": "comment.line.percentage.pdf",
					"match": "%.*"
				},
				{
					"name": "keyword.control.true.pdf",
					"match": "\\btrue\\b"
				},
				{
					"name": "keyword.control.reference.pdf",
					"match": "\\b\\d+ \\d+ R\\b"
				},
				{
					"name": "punctuation.definition.dictionary.end.pdf",
					"match": ">>"
				},
				{
					"name": "variable.other.pdf",
					"match": "/[^\\s]*"
				}
			]
		},
		{
			"name": "punctuation.definition.array.pdf",
			"begin": "\\[",
			"end": "\\]",
			"patterns": [
				{
					"name": "comment.line.percentage.pdf",
					"match": "%.*"
				},
				{
					"name": "keyword.control.special.pdf",
					"match": "\\b(?:true|false|null)\\b"
				}
			]
		},
		{
			"name": "variable.other.pdf",
			"match": "/[^\\s]*"
		},
		{
			"name": "constant.numeric.real.pdf",
			"match": "\\b[-+]?\\d*\\.\\d+\\b"
		},
		{
			"name": "constant.numeric.integer.pdf",
			"match": "\\b[+-]?\\d+\\b"
		},
		{
			"name": "meta.content-stream.pdf",
			"begin": "\\bstream\\b",
			"end": "\\bendstream\\b",
			"patterns": [
				{
					"name": "comment.line.percentage.pdf",
					"match": "%.*"
				},
				{
					"name": "binary.data.pdf",
					"begin": "\\bID\\b",
					"end": "\\bEI\\b",
					"contentName": "text.plain"
				},
				{
					"name": "keyword.operator.content-stream.pdf",
					"match": "\\b(w|J|j|M|d|ri|i|gs|q|Q|cm|cs|CS|SC|SCN|G|g|RG|rg|K|k|m|l|c|v|y|h|re|S|s|f|F|f\\*|B|B\\*|b|b\\*|n|W|W\\*|sh|BI|ID|EI|Do|Tc|Tw|Tz|TL|Tf|Tr|Ts|Td|TD|Tm|T\\*|do|d1|MP|DP|BMC|BDC|EMC|BX|EX|BT|ET|Tj|TJ|Tf|Tc|Tw|Td|cm)\\b"
				}
			]
		},
		{
			"name": "markup.underline.link.pdf",
			"match": "\\b(http|https)://[a-zA-Z0-9\\-.]+(/[a-zA-Z0-9\\-._?,'+&%$#=~]*)*\\b"
		},
		{
			"name": "keyword.section.xref-trailer.pdf",
			"begin": "\\bxref\\b",
			"end": "\\btrailer\\b",
			"patterns": [
				{
					"name": "keyword.control.f.pdf",
					"match": "\\bf\\b"
				},
				{
					"name": "keyword.control.n.pdf",
					"match": "\\bn\\b"
				}
			]
		},
		{
			"name": "keyword.control.stream.pdf",
			"match": "\\b(?:stream|endstream)\\b"
		}
	],
	"repository": {
		"main": {
			"patterns": [
				{
					"include": "#main"
				}
			]
		}
	}
}
