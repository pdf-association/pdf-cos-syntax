{
	"name": "PDF",
	"scopeName": "source.pdf",
	"patterns": [
		{
			"name": "string.quoted.pdf",
			"begin": "\\(",
			"end": "\\)",
			"patterns": [
				{
					"name": "constant.character.escape.octal.pdf",
					"match": "\\\\\\d{3}(?=[^\\d]|$)"
				},
				{
					"name": "constant.character.escape.newline.pdf",
					"match": "\\\\n"
				},
				{
					"name": "constant.character.escape.return.pdf",
					"match": "\\\\r"
				},
				{
					"name": "constant.character.escape.tab.pdf",
					"match": "\\\\t"
				},
				{
					"name": "constant.character.escape.backslash.pdf",
					"match": "\\\\\\\\"
				},
				{
					"name": "constant.character.escape.other.pdf",
					"match": "\\\\."
				}
			]
		},
		{
			"name": "comment.line.percentage.pdf",
			"match": "%.*"
		},
		{
			"name": "keyword.control.pdf",
			"match": "\\b(?:\\d+ \\d+ R|startxref|xref|trailer|null|true|false|^\\d+ \\d+ obj|^endobj|stream|endstream)\\b"
		},
		{
			"name": "keyword.section.xref-trailer.pdf",
			"begin": "\\bxref\\b",
			"end": "\\btrailer\\b",
			"patterns": [
				{
					"name": "keyword.control.f.pdf",
					"match": "\\bf\\b"
				},
				{
					"name": "keyword.control.n.pdf",
					"match": "\\bn\\b"
				}
			]
		},
		{
			"name": "punctuation.definition.dictionary.begin.pdf",
			"match": "<<"
		},
		{
			"name": "punctuation.definition.dictionary.end.pdf",
			"match": ">>"
		},
		{
			"name": "punctuation.definition.array.begin.pdf",
			"match": "\\["
		},
		{
			"name": "punctuation.definition.array.end.pdf",
			"match": "\\]"
		},
		{
			"name": "variable.other.pdf",
			"match": "/[^\\s]*"
		},
		{
			"name": "string.quoted.pdf",
			"match": "<[^>]*>"
		},
		{
			"name": "constant.numeric.real.pdf",
			"match": "[+-]?\\d+\\.\\d*\\b"
		},
		{
			"name": "constant.numeric.integer.pdf",
			"match": "[+-]?\\d+\\b"
		},
		{
			"name": "markup.underline.link.pdf",
			"match": "\\b(http|https)://[a-zA-Z0-9\\-.]+(/[a-zA-Z0-9\\-._?,'+&%$#=~]*)*\\b"
		},
		{
			"name": "string.quoted.double.pdf",
			"begin": "\\(",
			"end": "\\)",
			"patterns": [
				{
					"name": "constant.character.escape.pdf",
					"match": "\\\\."
				}
			]
		},
		{
			"name": "string.quoted.hex.pdf",
			"begin": "<",
			"end": ">",
			"patterns": [
				{
					"name": "constant.character.escape.pdf",
					"match": "\\\\."
				},
				{
					"name": "string.hex.pdf",
					"match": "[0-9A-Fa-f\\s]+"
				}
			]
		}
	],
	"repository": {
		"main": {
			"patterns": [
				{
					"include": "#main"
				}
			]
		}
	}
}
